{
  "name": "e2e-automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "e2e-stage": "NODE_ENV=stage wdio run test/configs/wdio.conf.ts",
    "e2e-generate-report": "allure generate --clean && allure open",
    "e2e-saucelab-only": "NODE_ENV=stage wdio run test/configs/saucelab.conf.ts --cucumberOpts.tagExpression=@only",
    "e2e-only": "NODE_ENV=stage wdio run test/configs/wdio.conf.ts --cucumberOpts.tagExpression=@only",
    "win-e2e-only": "SET NODE_ENV=stage&&wdio run test/configs/wdio.conf.ts --cucumberOpts.tagExpression=@only",
    "e2e-regression": "NODE_ENV=stage wdio run test/configs/regressionTests.conf.ts --cucumberOpts.tagExpression=@regression",
    "win-e2e-regression": "SET NODE_ENV=stage&&wdio run test/configs/regressionTests.conf.ts --cucumberOpts.tagExpression=@regression",
    "win-e2e-saucelabs-only": "SET NODE_ENV=stage&&wdio run test/configs/saucelab.conf.ts --cucumberOpts.tagExpression=@only",
    "win-e2e-lambdatest-only": "SET NODE_ENV=stage&&wdio run test/configs/lambdatest.conf.ts --cucumberOpts.tagExpression=@only",
    "e2e-lambdatest-only": "NODE_ENV=stage wdio run test/configs/lambdatest.conf.ts --cucumberOpts.tagExpression=@only",
    "specreporter-generate-report": "NODE_ENV=stage wdio run test/configs/specReporter.conf.ts --cucumberOpts.tagExpression=@only",
    "specreporter": "wdio run test/configs/specReporter.conf.ts",
    "htmlreporter": "wdio run test/configs/specReporter.conf.ts",
    "report": "node node_modules/wdio-html-nice-reporter/lib/makeReport.js master-report.html 'reports/html-reports/'"

  },
